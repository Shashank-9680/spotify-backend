{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" }
  ],

  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/auth(.*)",
      "dest": "index.js"
    },
    {
      "src": "/song(.*)",
      "dest": "index.js"
    },
    {
      "src": "/playlist(.*)",
      "dest": "index.js"
    },
    {
      "src": "/likedsongs(.*)",
      "dest": "index.js"
    }
   
    
  ],
  "rewrites": [{ "source": "/(.*)", "destination": "index.js" }]

  
}
